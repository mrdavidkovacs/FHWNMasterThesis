journal = {Journal of Computational Science},
keywords = {Collective mood,Sentiment tracking,Social networks,Stock market},
number = {1},
pages = {1--8},
pmid = {22173204},
title = {{Twitter mood predicts the stock market}},
volume = {2},
year = {2011}
}
