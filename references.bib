@phdthesis{Baudrexl2013,
abstract = {Crowdsourcing is the outsourcing of human intelligence tasks to a crowd composed by the general public, through open calls, via web-based platforms. The major obstacles to the success of crowdsourcing are quality issues of the delivered work due to various reasons. To overcome these quality issues, quality management measures need to be put in place. In the thesis at hand we rst examine the crowdsourcing paradigm and deduce quality issues. In the further course we identify various di erent measures for quality planning, assurance and control. In collaboration with an industry partner the practical application of speci c quality management measures is examined. Based on data provided through the crowdsourcing platform operated by the industry partner, an empirical investigation on the eciency of the methods in place is conducted. With our work, we provide a thorough insight into the crowdsourcing paradigm, its quality management issues and quality management measures to overcome these issues, from both a theoretical and practical point of view.},
author = {Baudrexl, Anton},
file = {:C$\backslash$:/Users/dkova/Documents/Mendeley Desktop/Baudrexl - 2013 - Quality Management for Crowdsourcing and its Practical Implication using the Example of an Industry Partner.pdf:pdf},
